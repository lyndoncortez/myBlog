<h1>Articles</h1>

<table>
  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= link_to 'Show', article %></td>
      </tr>
   <% end %> 
  </tbody>
</table>

<button><%= link_to "New Article", new_article_path, :class => "btn" %></button>

